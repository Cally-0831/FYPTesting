<script>

    async function submitform(startday,endday,userid,type){
    var start = new Date(startday);
    var end = new Date(endday);
    var today = start;
    var finalbody=[];
    
    if(parseInt(currenthour)<10){currenthour = "0"+parseInt(currenthour)}
    for(var a = 0 ; a < 6; a++){
       
        var followingDay = new Date(parseInt(start.getTime()) + a *24*60*60*1000);
        var today2 = followingDay.toLocaleDateString().split("/");
        
       
        if(parseInt(today2[1])<10){today2[1] = "0"+today2[1]};
        if(parseInt(today2[2])<10){today2[2] = "0"+today2[2]};
        var currenthour = 8;
        var currentmin = "30";
        var ans ="";

        for(var b = 1 ; b < 22; b++){
            var newCampus;
            var newRID;
            if(b%2 == 0 && b!=0 ){
                currentmin = "00"
                currenthour++;
            }else{
                currentmin = "30"
            };
            if(parseInt(currenthour)<10){currenthour="0"+parseInt(currenthour)};
        var boxid = today2[0]+""+today2[1]+""+today2[2]+"_"+currenthour+":"+currentmin+"_"+userid;
        
        if(document.getElementById(""+boxid+"_Obs") != null){
            var stringstring = document.getElementById(""+boxid+"_Obs").innerText.split("\n");
            ans ="";
           
            
            for(var c =0 ;c < stringstring.length ; c ++){
                if(c != 0){
                    var stringstring2= stringstring[c].split("-");
                    ans += "/"+stringstring2[0]
                }else{
                    ans += stringstring[c]
                }
            }

        }


       


        if(document.getElementById(""+boxid+"_Stu") != null){
            
            if(document.getElementById(""+boxid+"_Campus") != null){
                newCampus = document.getElementById(""+boxid+"_Campus").innerText;
                newRID = document.getElementById(""+boxid+"_RID").innerText;
            }else{
                console.log(boxid+"    "+document.getElementById(""+boxid+"_CampusSelectbox").value)
                if(document.getElementById(""+boxid+"_CampusSelectbox").value == "--Please Select--"){
                    return alert("Please Select A Room For "+today2[0]+"/"+today2[1]+"/"+today2[2]+" "+currenthour+":"+currentmin);
                }else{
                    newCampus = document.getElementById(""+boxid+"_CampusSelectbox").value;
                    if(document.getElementById(""+boxid+"_RIDSelectbox").value == "--Please Select--"){
                        return alert("Please Select A Room For "+today2[0]+"/"+today2[1]+"/"+today2[2]+" "+currenthour+":"+currentmin);
                    }else{
                        newRID = document.getElementById(""+boxid+"_RIDSelectbox").value;
                    }
                }
            }

            
            
       
            var thisbox = {
                'boxid':boxid,
                'boxdate':today2[0]+""+today2[1]+""+today2[2],
                'boxtime':currenthour+":"+currentmin+":00",
                'stu':document.getElementById(""+boxid+"_Stu").innerText,
